#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/import-local@3.0.3/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../fixtures/cli.js" "$@"
fi
