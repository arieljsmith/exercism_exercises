#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/sane@4.1.0/node_modules/sane/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/sane@4.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@cnakazawa+watch@1.0.4/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/sane@4.1.0/node_modules/sane/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/sane@4.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@cnakazawa+watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
fi
