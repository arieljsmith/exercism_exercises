#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules/semver/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/core-js-compat@3.20.1/node_modules/core-js-compat/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/core-js-compat@3.20.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules/semver/bin/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules/semver/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/semver@7.0.0/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/core-js-compat@3.20.1/node_modules/core-js-compat/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/core-js-compat@3.20.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@7.0.0/node_modules/semver/bin/semver.js" "$@"
fi
