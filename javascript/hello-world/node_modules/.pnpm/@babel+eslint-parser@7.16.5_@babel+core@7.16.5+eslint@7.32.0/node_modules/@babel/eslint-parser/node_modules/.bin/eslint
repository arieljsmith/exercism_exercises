else
  export NODE_PATH="$NODE_PATH:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/eslint@7.32.0/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/node_modules:/Users/arieljsmith/Exercism/node_modules:/Users/arieljsmith/node_modules:/Users/node_modules:/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@babel+eslint-parser@7.16.5_@babel+core@7.16.5+eslint@7.32.0/node_modules/@babel/eslint-parser/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@babel+eslint-parser@7.16.5_@babel+core@7.16.5+eslint@7.32.0/node_modules/@babel/node_modules:/Users/arieljsmith/Exercism/exercism_exercises/javascript/hello-world/node_modules/.pnpm/@babel+eslint-parser@7.16.5_@babel+core@7.16.5+eslint@7.32.0/node_modules"
